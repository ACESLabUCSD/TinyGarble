		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "(A^B)'";
			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell (DFF){
		area : 0;
		ff(IQ,IQN){
			next_state : "D";
			clocked_on : "CLK";
			clear : "RST";
		}
		pin(D){
			direction : input;
			capacitance : 1;
			timing() {
			  related_pin : "CLK";
			}
		}
		pin(CLK){
			direction : input;
			capacitance : 1;
			clock : true;
		}
		pin(RST){
			direction : input;
			capacitance : 1;
		}
		pin(Q){
			direction : output;
			function : "IQ";
			timing() {
			  related_pin : "CLK";
			  timing_type : falling_edge;
			}
			timing() {
				related_pin : "RST";
				timing_type : clear;
				timing_sense : positive_unate;
			}
		}
	}
}
