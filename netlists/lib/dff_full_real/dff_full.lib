library(dff_nor_xor){

	/* unit attributes */
	time_unit : "1ns";
	voltage_unit : "1V";
	current_unit : "1uA";
	pulling_resistance_unit : "1kohm";
	leakage_power_unit : "1nW";
	capacitive_load_unit (1,pf);
  
	cell(AND){
		area : 6;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "AB";
			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell(ANDN){
		area : 6;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "A'B";
			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell(NAND){
		area : 4;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "(AB)'";
			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell(NANDN){
		area : 6;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "(A'B)'";
			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell(OR){
		area : 6;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "(A+B)";
			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell(ORN){
		area : 6;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "(A'+B)";
			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell(NOR){
		area : 4;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "(A+B)'";
			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell(NORN){
		area : 6;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
