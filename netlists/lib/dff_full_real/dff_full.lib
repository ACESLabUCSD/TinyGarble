			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell(IV){
		area : 2;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "A'";
			timing(){
				related_pin : "A";
			}
		}
	}

	cell(XOR){
		area : 8;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "A^B";
			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell(XNOR){
		area : 8;
		pin(A){
			direction : input;
