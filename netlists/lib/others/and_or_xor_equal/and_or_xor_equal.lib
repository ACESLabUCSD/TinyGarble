library(and_or_xor_equal) {
	/* unit attributes */
	time_unit : "1ns";
	voltage_unit : "1V";
	current_unit : "1uA";
	pulling_resistance_unit : "1kohm";
	leakage_power_unit : "1nW";
	capacitive_load_unit (1,pf);
	
cell(AND) {
  area : 2;
  pin(A) {
    direction : input;
    capacitance : 1;
  }
  pin(B) {
    direction : input;
    capacitance : 1;
  }
  pin(Z) {
    direction : output;
    function : "A B";
	timing() {
      intrinsic_rise : 48;
      intrinsic_fall : 77;
      rise_resistance : 14.43;
      fall_resistance : 5.23;
      slope_rise : 0.0;
      slope_fall : 0.0;
      related_pin : "A";
    }
    timing() {
      intrinsic_rise : 48;
      intrinsic_fall : 77;
      rise_resistance : 14.43;
      fall_resistance : 5.23;
      slope_rise : 0.0;
      slope_fall : 0.0;
      related_pin : "B";
    }
  }
}

