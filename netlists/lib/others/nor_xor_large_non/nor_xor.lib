library(dff_nor_xor){
	/* unit attributes */
	time_unit : "1ns";
	voltage_unit : "1V";
	current_unit : "1uA";
	pulling_resistance_unit : "1kohm";
	leakage_power_unit : "1nW";
	capacitive_load_unit (1,pf);
	cell(NOR){
		area : 5;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "(A+B)'";
			timing(){
			      intrinsic_rise : 0.48;
			      intrinsic_fall : 0.77;
			      rise_resistance : 0.1443;
			      fall_resistance : 0.0523;
			      slope_rise : 0.0;
			      slope_fall : 0.0;
			      related_pin : "A";
			}
			timing(){
			      intrinsic_rise : 0.48;
			      intrinsic_fall : 0.77;
			      rise_resistance : 0.1443;
			      fall_resistance : 0.0523;
			      slope_rise : 0.0;
			      slope_fall : 0.0;
			      related_pin : "B";
			}
		}
	}

	cell(IV){
		area : 1;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "A'";
			timing(){
				intrinsic_rise : 0.48;
				intrinsic_fall : 0.77;
				rise_resistance : 0.1443;
				fall_resistance : 0.0523;
				slope_rise : 0.0;
				slope_fall : 0.0;
				related_pin : "A";
			}
		}
	}

	cell(XOR){
		area : 1;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "A^B";
			timing(){
				intrinsic_rise : 0.48;
				intrinsic_fall : 0.77;
				rise_resistance : 0.1443;
				fall_resistance : 0.0523;
				slope_rise : 0.0;
				slope_fall : 0.0;
				related_pin : "A";
			}
			timing(){
				intrinsic_rise : 0.48;
				intrinsic_fall : 0.77;
				rise_resistance : 0.1443;
				fall_resistance : 0.0523;
				slope_rise : 0.0;
				slope_fall : 0.0;
				related_pin : "B";
			}
		}
	}
}
