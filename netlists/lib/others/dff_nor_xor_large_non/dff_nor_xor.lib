library(dff_nor_xor){

	/* unit attributes */
	time_unit : "1ns";
	voltage_unit : "1V";
	current_unit : "1uA";
	pulling_resistance_unit : "1kohm";
	leakage_power_unit : "1nW";
	capacitive_load_unit (1,pf);
  
	cell(NOR){
		area : 5;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "(A+B)'";
			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell(IV){
		area : 1;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "A'";
			timing(){
				related_pin : "A";
			}
		}
	}

	cell(XOR){
		area : 1;
		pin(A){
			direction : input;
			capacitance : 1;
		}
		pin(B){
			direction : input;
			capacitance : 1;
		}
		pin(Z){
			direction : output;
			function : "A^B";
			timing(){
				related_pin : "A";
			}
			timing(){
				related_pin : "B";
			}
		}
	}

	cell (DFF){
		area : 0;
		ff(IQ,IQN){
			next_state : "D";
			clocked_on : "CLK";
			clear : "RST";
		}
		pin(D){
			direction : input;
			capacitance : 1;
			timing() {
			  related_pin : "CLK";
			}
		}
		pin(CLK){
			direction : input;
			capacitance : 1;
			clock : true;
		}
		pin(RST){
			direction : input;
			capacitance : 1;
		}
		pin(Q){
			direction : output;
			function : "IQ";
			timing() {
			  related_pin : "CLK";
			  timing_type : falling_edge;
			}
			timing() {
				related_pin : "RST";
				timing_type : clear;
				timing_sense : positive_unate;
			}
		}
	}
}
